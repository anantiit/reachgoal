    <File name="jsonLogger" fileName="/Users/appalanaiduabotula/logsHackathon/logJson.log">

      <JSONLayout compact="true"/>

    </File>
    <File name="debugLogger" fileName="/Users/appalanaiduabotula/logsHackathon/logDebug.log">

